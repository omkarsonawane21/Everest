<UpdateAundhBill>:
    BoxLayout:
        id: AfterLogin_Layout
        orientation: 'vertical'
        spacing: 20

        MDToolbar:
            title:"Aundh Update Bill"
            left_action_items: [["logout",lambda x: root.disconnect()]]
            right_action_items: [["dots-vertical",lambda x: app.navigation_draw()]]
            elevation: 8
            font_size: 16
        Widget:

    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        type:['content']
        pos_hint: {"center_y": .394,"center_x": .52}
        BoxLayout:
            orientation: 'vertical'
            padding: [10,20,20,300]
            size_hint_y:None
            height: self.minimum_height
            spacing: 20
            BoxLayout:
                orientation: 'vertical'
                size_hint_y:None
                height: self.minimum_height
                pos_hint: {"center_x": .5,"center_y": .5}
                spacing:10

                MDTextField:
                    hint_text: "Enter Retailer Name"
                    required: True
                    helper_text_mode: "on_error"
                    helper_text: "No Retailer Name"
                    id: aunretailername
                    multiline:False
                    font_size: '20sp'
                    font_name: 'Mukta-SemiBold'
                    background_color: 1,1,1,0.5
                    size_hint_y:None
                    size:
                        self.texture_size
                    size: dp(260),dp(40)
                    on_text:self.text = self.text.upper()

                MDLabel:
                    id: search_aun_retailer  #retailer does not exist
                    font_name: 'VarelaRound'
                    font_size: '15sp'
                    text_size: root.width-14, 14
                    color: rgba("#0e1574ff")
                HoSeparator

                Spinner:
                    id: aunspinner
                    size_hint: None, None
                    size: dp(300),dp(40)
                    pos_hint: {'center': (.5, .5)}
                    text: 'Retailer List'
                    on_text: aunretailername.text = self.text

                GridLayout:
                    cols: 2
                    spacing:10

                    MDRaisedButton:
                        valign:'center'
                        text: 'Search Retailer'
                        font_size: '16sp'
                        font_name: 'VarelaRound'
                        on_press: root.search_aun_ret(aunretailername.text)

                    MDRaisedButton:
                        valign:'center'
                        text: 'Print'
                        font_size: '16sp'
                        font_name: 'VarelaRound'
                        on_press: root.printret(aunretailername.text)
                HoSeparator
                HoSeparator
                HoSeparator

                MDTextField:
                    hint_text: "Enter Masala Name"
                    required: True
                    helper_text_mode: "on_error"
                    helper_text: "No Masala Name"
                    id: aunmasalaname
                    multiline:False
                    font_size: '20sp'
                    font_name: 'Mukta-SemiBold'
                    background_color: 1,1,1,0.5
                    size_hint_y:None
                    size:
                        self.texture_size
                    size: dp(260),dp(40)
                    on_text:self.text = self.text.upper()

                MDLabel:
                    id: search_masala   #masala not exist
                    font_name: 'VarelaRound'
                    font_size: '15sp'
                    text_size: root.width-14, 14
                    color: rgba("#0e1574ff")

                MDTextField:
                    hint_text: "Enter Return Quantity"
                    required: True
                    helper_text_mode: "on_error"
                    helper_text: "No Quantity Entered"
                    id: aunquantity
                    input_filter: 'int'
                    multiline:False
                    font_size: '20sp'
                    font_name: 'Mukta-SemiBold'
                    background_color: 1,1,1,0.5
                    size_hint_y:None
                    size:
                        self.texture_size
                    size: dp(260),dp(40)

                Spinner:
                    id: masspinner
                    size_hint: None, None
                    size: dp(300),dp(40)
                    pos_hint: {'center': (.5, .5)}
                    text: 'Masala List'
                    on_text: aunmasalaname.text = self.text


                GridLayout:
                    cols: 2
                    spacing:10

                    MDRaisedButton:
                        valign:'center'
                        text: 'Search Masala'
                        font_size: '16sp'
                        font_name: 'VarelaRound'
                        on_press: root.searchmasala(aunmasalaname.text)

                    MDRaisedButton:
                        valign:'center'
                        text: 'Return Masala'
                        font_size: '16sp'
                        font_name: 'VarelaRound'
                        on_press: root.searchmasforaundh(aunmasalaname.text, aunquantity.text, aunretailername.text)

                    MDRaisedButton:
                        valign:'center'
                        text: 'Expired Masala'
                        font_size: '16sp'
                        font_name: 'VarelaRound'
                        on_press: root.aun_expiry(aunmasalaname.text, aunquantity.text, aunretailername.text)

                    MDRaisedButton:
                        valign:'center'
                        text: 'Submit Retailer'
                        font_size: '16sp'
                        font_name: 'VarelaRound'
                        on_press: root.aun_ret_submit(aunretailername.text)

                    MDRaisedButton:
                        valign:'center'
                        text: 'Save'
                        font_size: '16sp'
                        font_name: 'VarelaRound'
                        on_press: root.save_aun_bill()

    MDBottomAppBar:
        MDToolbar:
            font_name: 'Mukta-Medium'
            mode:'end'
            type:'bottom'
            icon:'arrow-left-bold'
            on_action_button: root.back()

