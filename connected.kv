#:import Snackbar kivymd.uix.snackbar.Snackbar

<Separator@Widget>:
    canvas:
        Color:
            rgba: 0, 0, 0, .4
        Rectangle:
            pos: self.pos
            size: self.size

<HSeparator@Separator>:
    size_hint_y: None
    height: dp(1)

<MySeparator@Widget>:
    canvas:
        Color:
            rgba: 0, 0, 0, 0
        Rectangle:
            pos: self.pos
            size: self.size

<HoSeparator@MySeparator>:
    size_hint_y: None
    height: dp(1)



<BoyConnected>:
    BoxLayout:
        id: AfterLogin_Layout
        orientation: 'vertical'
        spacing: 20

        MDToolbar:
            title:"EVEREST SPICES"
            left_action_items: [["logout",lambda x: root.disconnect()]]
            right_action_items: [["dots-vertical",lambda x: app.navigation_draw()]]
            elevation: 8
            font_size: 16
        Widget:

    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        type:['content']
        pos_hint: {"center_y": .394 ,"center_x": .52}
        BoxLayout:
            orientation: 'vertical'
            padding: [10,30,25,150]
            size_hint_y:None
            height: self.minimum_height
            spacing: 20

            #View Bill
            MDLabel:
                text: 'View Bill'
                font_name: 'VarelaRound'
                font_size: '20sp'
                color: rgba("#0e1574ff")

            BoxLayout:
                orientation: 'vertical'
                size_hint_y:None
                height: self.minimum_height
                pos_hint: {"center_x": .7,"center_y": .5}
                spacing:10
                MDRaisedButton:
                    valign:'center'
                    text: 'Aundh'
                    font_size: '20sp'
                    font_name: 'VarelaRound'
                    md_bg_color: 1, 0, 1, 1
                    on_press: root.viewAunBills()
                MDRaisedButton:
                    valign:'center'
                    text: 'Balewadi'
                    font_size: '20sp'
                    font_name: 'VarelaRound'
                    md_bg_color: 1, 0, 1, 1
                    on_press: root.viewBalBills()
                MDRaisedButton:
                    valign:'center'
                    text: 'Sangvi'
                    font_size: '20sp'
                    font_name: 'VarelaRound'
                    on_press: root.viewSanBills()
            HSeparator
            #Update Bill
            MDLabel:
                text: 'Update Bill'
                font_name: 'VarelaRound'
                font_size: '20sp'
                text_size: root.width-14, 14
                color: rgba("#0e1574ff")

            BoxLayout:
                orientation: 'vertical'
                size_hint_y:None
                height: self.minimum_height
                pos_hint: {"center_x": .7,"center_y": .5}
                spacing:10
                MDRaisedButton:
                    valign:'center'
                    text: 'Aundh'
                    font_size: '20sp'
                    font_name: 'VarelaRound'
                    on_press: root.updateaunbill()
                MDRaisedButton:
                    valign:'center'
                    text: 'Balewadi'
                    font_size: '20sp'
                    font_name: 'VarelaRound'
                    on_press: root.updatebalbill()
                MDRaisedButton:
                    valign:'center'
                    text: 'Sangvi'
                    font_size: '20sp'
                    font_name: 'VarelaRound'
                    on_press: root.updatesanbill()
            HSeparator

            #Delivery of masala
            MDLabel:
                text: 'Delivery'
                font_name: 'VarelaRound'
                font_size: '20sp'
                text_size: root.width-14, 14
                color: rgba("#0e1574ff")

            BoxLayout:
                orientation: 'vertical'
                size_hint_y:None
                height: self.minimum_height
                pos_hint: {"center_x": .7,"center_y": .5}
                spacing:10
                MDRaisedButton:
                    valign:'center'
                    text: 'Aundh'
                    font_size: '20sp'
                    font_name: 'VarelaRound'
                    on_press: root.aundelivery()
                MDRaisedButton:
                    valign:'center'
                    text: 'Balewadi'
                    font_size: '20sp'
                    font_name: 'VarelaRound'
                    on_press: root.baldelivery()
                MDRaisedButton:
                    valign:'center'
                    text: 'Sangvi'
                    font_size: '20sp'
                    font_name: 'VarelaRound'
                    on_press: root.sandelivery()
    MDBottomAppBar:
        MDToolbar:
            font_name: 'Mukta-Medium'
            mode:'end'
            type:'bottom'
            icon:'arrow-right-bold'
            on_action_button: root.next()

<BoyViewAunBills>:
    BoxLayout:
        orientation: 'vertical'
        spacing: 20

        MDToolbar:
            title:"Bills"
            elevation: 8
            font_size: 16
        Widget:

    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        type:['content','bars']
        pos_hint: {"center_y": .39,"center_x": .52}
        BoxLayout:
            orientation: 'vertical'
            padding: [10,20,10,150]
            size_hint_y:None
            height: self.minimum_height

            MDCard:
                orientation: "vertical"
                pos_hint:{ "center_x" :0.5, "center_y": 0.5}
                padding: "8dp"
            MDLabel:
                id: mylabel
                font_size: '14sp'
                font_name: 'VarelaRound'
                size_hint_x:1.0
                size_hint_y:None
                text_size: self.width,None
                height: self.texture_size[1]
    MDBottomAppBar:
        MDToolbar:
            font_name: 'Mukta-Medium'
            mode:'end'
            type:'bottom'
            icon:'arrow-left-bold'
            on_action_button: root.back()

